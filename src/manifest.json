{
  "manifest_version": 3,
  "name": "Playlist Searcher for YouTube Music",
  "version": "1.0.0",
  "description": "Search for playlists in YouTube Music website by playlist name.",
  "host_permissions": [
    "https://music.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://music.youtube.com/*"
      ],
      "js": [
        "utils.js",
        "templates.js",
        "playlist-dialog-observer.js",
        "dialog.js",
        "sidebar.js"
      ],
      "css": [
        "ytmps-styles.css"
      ]
    }
  ],
  "icons": {
    "16": "img/logo16x16.png",
    "32": "img/logo32x32.png",
    "48": "img/logo48x48.png",
    "64": "img/logo64x64.png",
    "128": "img/logo128x128.png",
    "512": "img/logo512x512.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "playlist-searcher-for-ytm@milano-slesarik"
    }
  }
}